<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    
    <servlet>
        <servlet-name>Registrar_proveedores</servlet-name>
        <servlet-class>servlet.Registrar_proveedores</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Registrar_proveedores</servlet-name>
        <url-pattern>/Registrar_proveedores</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>IniciarSesion</servlet-name>
        <servlet-class>servlet.IniciarSesion</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>IniciarSesion</servlet-name>
        <url-pattern>/IniciarSesion</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Registrar_usuarios</servlet-name>
        <servlet-class>servlet.Registrar_usuarios</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Registrar_usuarios</servlet-name>
        <url-pattern>/Registrar_usuarios</url-pattern>
    </servlet-mapping>
    
    
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>


